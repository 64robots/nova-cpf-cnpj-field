{
  "name": "r64/nova-cpf-cnpj-field",
  "description": "Brazilian CPF and CNPJ Field for Laravel Nova",
  "keywords": [
    "laravel",
    "nova",
    "cpf",
    "cnpj"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Michiel van der Geest",
      "email": "hello@michielvandergeest.com"
    }
  ],
  "require": {
    "php": ">=7.1.0"
  },
  "autoload": {
    "psr-4": {
      "R64\\NovaCpfCnpjField\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "R64\\NovaCpfCnpjField\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
